{
    "node": true,
    "browser": false,
    "esnext": true,
    "strict": true,
    "bitwise": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "eqnull": true,
    "forin": true,
    //"es3": false,
    "freeze": true,
    "funcscope": true,
    "immed": true,
    "indent": 2,
    "latedef": false,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    //"maxparams": 4,
    //"maxdepth": 3,
    "maxlen": 120,
    "debug": true,
    "shadow": true,
    "predef": [ "Promise" ]
}
